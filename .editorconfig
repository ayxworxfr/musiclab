# EditorConfig 帮助开发者在不同编辑器之间保持一致的编码风格
# https://editorconfig.org

root = true

[*]
charset = utf-8
indent_style = space
indent_size = 2
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = true

[*.md]
trim_trailing_whitespace = false

[*.dart]
indent_size = 2

[*.{yaml,yml}]
indent_size = 2

[Makefile]
indent_style = tab

